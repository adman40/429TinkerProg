{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug hw5",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/hw5",
            "args": [
                "input.tk",
                "output.tko"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Compile hw5"
        }
    ]
}
